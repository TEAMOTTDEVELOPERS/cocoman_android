<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@drawable/checkboxchecked" android:state_checked="true">
        <shape
            android:shape="rectangle">
            <size
                android:width="20dp"
                android:height="20dp"/>
            <solid
                android:color="#00FFFFFF"/>
        </shape>
    </item>
    <item android:drawable="@drawable/checkboxempty" android:state_checked="false" android:layout_width="10dp"
        android:layout_height="10dp" />
</selector>